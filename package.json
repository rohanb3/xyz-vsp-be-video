{
  "name": "xyzies-vsp-be",
  "version": "1.0.0",
  "description": "SimpleBE",
  "main": "index.js",
  "scripts": {
    "start": "npm run copyEnv && npm run clean-public && npm run build-public && node index.js",
    "start:dev": "npm run clean-public && npm run build-public && nodemon index.js",
    "build-public": "mkdir -p src/public/js && browserify src/public/customer.js > src/public/js/customer.js",
    "clean-public": "rimraf src/public/js/customer.js",
    "check": "npm run lint && npm run unit",
    "lint": "npx eslint --fix src/**/*.js tests/**/*.js",
    "unit": "jest",
    "coverage": "jest --collectCoverageFrom=src/**/*.js --coverage",
    "copyEnv": "cp .env.template .env"
  },
  "author": "Ketler",
  "license": "ISC",
  "dependencies": {
    "ansi-regex": "^4.1.0",
    "body-parser": "^1.18.3",
    "config": "^3.0.1",
    "connect-redis": "^3.4.0",
    "cors": "^2.8.5",
    "cycle": "^1.0.3",
    "dotenv": "^6.1.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "express-validator": "^5.3.1",
    "helmet": "^3.15.0",
    "module-alias": "^2.1.0",
    "moment": "^2.22.2",
    "mongoose": "^5.4.6",
    "mongoose-beautiful-unique-validation": "^7.1.1",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.0",
    "passport-azure-ad": "^4.0.0",
    "redis": "^2.8.0",
    "save": "^2.3.3",
    "socket.io": "^2.1.1",
    "socket.io-redis": "^5.2.0",
    "socketio-auth": "^0.1.1",
    "swagger-ui-express": "^4.0.2",
    "triple-beam": "^1.3.0",
    "twilio": "^3.24.0",
    "twilio-video": "^2.0.0-beta8",
    "winston": "^3.1.0",
    "winston-compat": "^0.1.4",
    "winston-logstash": "^0.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "browserify": "^16.2.3",
    "copyfiles": "^2.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "redis-mock": "^0.43.0",
    "rimraf": "^2.6.2",
    "supertest": "^3.4.2"
  },
  "_moduleAliases": {
    "@": "./src"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check"
    }
  }
}
